{
  "name": "ZolaExample",
  "version": "1.0.0",
  "description": "This is an example project for Zola.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/MisterMChem/ZolaExample.git"
  },
  "homepage": "https://zola.com",
  "scripts": {
    "start": "npm run dev",
    "dev": "npm-run-all development:client:rendering-service:build:prepare development",
    "development": "npm-run-all --parallel development:client:build development:client:rendering-service:build development:services",
    "development:client:rendering-service:build:prepare": "universal-webpack --settings ./webclient/webpack/universal-webpack-settings.js --config ./webclient/webpack/webpack.config.js prepare",
    "development:client:build": "babel-node ./webclient/webpack/webpack-dev-server.js",
    "development:client:rendering-service:build": "webpack --config ./webclient/webpack/webpack.config.server.development.babel.js --watch --colors --display-error-details",
    "development:api": "nodemon --inspect=4003 ./api/entry.js --watch ./api",
    "development:client:rendering-service": "nodemon --inspect=4002 ./webclient/rendering-service/entry.js --watch ./webclient/rendering-service --watch ./webclient/build/server",
    "development:client:rendering-service:delayed": "npm-run-all delay development:client:rendering-service",
    "development:webserver": "nodemon ./webserver/entry.js --watch ./webserver",
    "development:webserver:delayed": "npm-run-all delay delay development:webserver",
    "development:services": "npm-run-all --parallel development:api development:client:rendering-service:delayed development:webserver:delayed",
    "delay": "node ./sleep 500",
    "production": "npm-run-all production:client:build production:client:rendering-service:build production:services",
    "production:client:build": "webpack --colors --display-error-details --config ./webclient/webpack/webpack.config.client.production.babel.js",
    "production:client:rendering-service:build": "webpack --colors --display-error-details --config ./webclient/webpack/webpack.config.server.production.babel.js",
    "production:api": "better-npm-run production:api",
    "production:client:rendering-service": "better-npm-run production:client:rendering-service",
    "production:webserver": "better-npm-run production:webserver",
    "production:services": "npm-run-all --parallel production:api production:client:rendering-service production:webserver"
  },
  "betterScripts": {
    "production:client:rendering-service": {
      "command": "node ./webclient/rendering-service/entry.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "production:api": {
      "command": "node ./api/entry.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "production:webserver": {
      "command": "node ./webserver/entry.js",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "author": "Halt Hammerzeit",
  "license": "MIT",
  "dependencies": {
    "autoprefixer": "^6.6.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-object-rest-spread": "^6.20.2",
    "babel-plugin-transform-react-display-name": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-plugin-typecheck": "^3.9.0",
    "babel-polyfill": "^6.20.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.18.0",
    "better-npm-run": "0.0.13",
    "bluebird": "^3.4.7",
    "body-parser": "^1.16.0",
    "classnames": "^2.2.5",
    "clean-webpack-plugin": "^0.1.14",
    "cookie-parser": "^1.4.3",
    "css-loader": "^0.26.1",
    "express": "^4.14.0",
    "express-session": "^1.14.2",
    "extract-text-webpack-plugin": "^2.0.0-beta.4",
    "file-loader": "^0.9.0",
    "jsonwebtoken": "^7.2.1",
    "koa-bodyparser": "^2.3.0",
    "koa-convert": "^1.2.0",
    "koa-cookie": "^1.0.0",
    "koa-mount": "^2.0.0",
    "koa-passport": "^1.4.0",
    "koa-route": "^2.4.2",
    "lodash": "^4.17.4",
    "node-sass": "^4.1.1",
    "npm-run-all": "^4.0.0",
    "passport": "^0.3.2",
    "passport-jwt": "^2.2.1",
    "postcss-calc": "^5.3.1",
    "postcss-custom-properties": "^5.0.1",
    "postcss-loader": "^1.2.1",
    "query-string": "^4.3.4",
